{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "A portfolio page",
  "scripts": {
    "begin": "npm run build && cd server && node server.js",
    "start": "cd server && node server.js",
    "start:original": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "cd server && node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emirodriguezalc/portfolio.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/emirodriguezalc/portfolio/issues"
  },
  "homepage": "https://github.com/emirodriguezalc/portfolio#readme",
  "dependencies": {
    "express": "^4.17.1",
    "heroku": "^7.47.11",
    "nodemailer": "^6.4.17",
    "path": "^0.12.7",
    "react-card-flip": "^1.0.11",
    "serve": "^11.3.2",
    "multiparty": "^4.2.2"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "react-card-flip": "^1.0.11",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "express": "^4.17.1",
    "multiparty": "^4.2.2",
    "nodemailer": "^6.4.17",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": "14.x"
  }
}
