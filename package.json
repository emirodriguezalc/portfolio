{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "A portfolio page",
  "scripts": {
    "begin": "npm run build && cd server && node server.js",
    "start": "cd server && node server.js",
    "start:original": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "cd server && node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emirodriguezalc/portfolio.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/emirodriguezalc/portfolio/issues"
  },
  "homepage": "https://github.com/emirodriguezalc/portfolio#readme",
  "dependencies": {
    "heroku": "^7.47.11",
    "path": "^0.12.7",
    "react-card-flip": "^1.0.11",
    "serve": "^11.3.2"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "react-card-flip": "^1.0.11"
  },
  "engines": {
    "node": "14.x"
  }
}
